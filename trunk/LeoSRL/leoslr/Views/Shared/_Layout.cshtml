<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    <link href='http://fonts.googleapis.com/css?family=Lato:100,400' rel='stylesheet' type='text/css'>
    @Styles.Render("~/bundles/css")
</head>
<body>
    @RenderBody()

    @Scripts.Render("~/bundles/jquery")
    
    @Scripts.Render("~/Scripts/jquery-ui.min.js")

    @Scripts.Render("~/Scripts/leo.js")

    @*@Scripts.Render("~/Scripts/data.js")*@

    @Scripts.Render("~/Scripts/init.js")
    
    @Scripts.Render("~/Scripts/jquery.bxslider.min.js")
    
    @Scripts.Render("~/Content/fancybox/jquery.fancybox-1.3.4.js")

    @RenderSection("scripts", required: false)
</body>
</html>
