<!DOCTYPE html>
<html>
<head>
    <title>@ViewBag.Title</title>
    <link href="@Url.Content("~/Content/Site.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/Admin.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Scripts/jquery-1.5.1.min.js")" type="text/javascript"></script>
    <meta name="description" content="@ViewBag.SeoDescription" />
    <meta name="keywords" content="@ViewBag.SeoKeywords" />
    @RenderSection("Includes", false)
</head>
<body>
    <div id="page">
        <div id="leftContent">
           
        </div>
        
        <div id="menuContainer">
        @Html.Partial("MainMenu")
        </div>
        <div id="content">

            
            @RenderSection("ContentTitle",false)

            
            
            @RenderBody()
        </div>
        <div id="footer">
        <div id="sign">Дизайн сайта<br/> &mdash;<br/> <a href="http://m-brand.com.ua">Угадайтекто</a>  </div>
        </div>
    </div>
</body>
</html>
