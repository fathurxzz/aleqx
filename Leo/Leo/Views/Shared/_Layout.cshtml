<!DOCTYPE html>
<html>
<head>
    <title>@ViewBag.Title</title>
    <link href="@Url.Content("~/Content/Site.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/Gallery.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Scripts/jquery-1.5.1.min.js")" type="text/javascript"></script>
    @if (Request.IsAuthenticated)
    {
        <link href="@Url.Content("~/Content/Admin.css")" rel="stylesheet" type="text/css" />
    }
    <meta name="description" content="@ViewBag.SeoDescription" />
    <meta name="keywords" content="@ViewBag.SeoKeywords" />
     @RenderSection("Includes", false)
</head>
<body>
    <div class="page">
        @RenderSection("MainMenu",false)
        <div class="boardContainer">
            <div class="board">
            </div>
            
                @RenderSection("CategoryTitle",false)
            
        </div>
        <div class="galleryContainer">

            @RenderSection("GalleryContent",false)

            
        </div>
        @RenderBody()
    </div>
    <div class="footerContainer">
        <div class="footer">
            <div class="top">
            </div>
            <div class="bottom">
                <div class="inner">
                    <a href="#">об авторе</a> <a href="#">обратная связь</a>
                </div>
            </div>
        </div>
    </div>
</body>
</html>
