@using HavilaTravel.Models
@model List<MenuItem>
@{

    
    
    
    var cnt = 0;
    if (Model.Count > 1)
    {
    <div id="placesMap">
        <table>
            <tr>
                @foreach (var item in Model.OrderBy(m => m.SortOrder))
                {
                    cnt++;
                    <td>
                        @if (cnt != Model.Count)
                        {
                            @Html.ActionLink(item.Title, "Index", "Place", new {id = item.Name}, null)
                            @Html.Raw("&nbsp;")
                            @Html.Raw("&raquo;")
                        }
                        @*else
                        {
                            @Html.Raw(item.Title)
                        }*@
                    </td>
                }
            </tr>
        </table>
    </div>
       
    }
}
